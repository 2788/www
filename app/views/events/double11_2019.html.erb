<%# 存储专场 %>
<div class="features features-double11-2019 features-double11-2019-kodo tab-content-double11-2019-kodo">
  <div class="container">
    <div class="intro">
      <h2 class="title">
        <%= image_tag "double11_2019/icon-title-red.png", title: I18n.t("views.events.double11_2019.kodo.title"), alt: I18n.t("views.events.double11_2019.kodo.title"), class: "icon-title left" -%>
        <%= I18n.t("views.events.double11_2019.kodo.title") -%>
        <%= image_tag "double11_2019/icon-title-red-blue.png", title: I18n.t("views.events.double11_2019.kodo.title"), alt: I18n.t("views.events.double11_2019.kodo.title"), class: "icon-title right" -%>
      </h2>
      <p class="desc"><%= raw(I18n.t("views.events.double11_2019.kodo.desc")) -%></p>
    </div>
    <div class="row">
      <% I18n.t("views.events.double11_2019.kodo.list").each do |kodo| -%>
      <div class="col-md-3 col-sm-6 col-xs-12 col-item col-kodo">
        <div class="thumbnail">
          <% if kodo[:isNew] -%>
          <div class="activity-flag">新人专享</div>
          <% end -%>
          <div class="caption">
            <h4 class="title"><%= kodo[:title] -%></h4>
            <p class="desc"><%= kodo[:desc] -%></p>
            <div class="content-container">
              <div class="content-dropdown">
                <p class="dropdown-title">地区</p>
                <select class="form-control double11-2019-dropdown double11-2019-kodo-dropdown">
                  <% kodo[:durations].each do |duration| -%>
                  <option value="<%= duration[:value] -%>"><%= duration[:text] -%></option>
                  <% end -%>
                </select>
              </div>
              <span class="activity-tip">双 11 狂欢</span>
              <% kodo[:sections].each do |section| -%>
              <section class="content-section <%= section[:class] -%>" id="<%= section[:id] -%>">
                <div class="content-price">
                  <div class="row">
                    <div class="col-md-5 col-sm-5 col-xs-5 col-price col-price-left">
                      <span class="span-red span-bold">￥</span><span class="span-red span-huge span-bold"><%= section[:price] -%></span>
                    </div>
                    <div class="col-md-7 col-sm-7 col-xs-7 col-price col-price-right">
                      <span class="span-red">节省&nbsp;<%= section[:save] -%>&nbsp;元</span>
                      <span class="span-red span-elide">原价&nbsp;<%= section[:originPrice] -%>&nbsp;元</span>
                    </div>
                  </div>
                </div>
                <span class="progress-tip">已抢 0%</span>
                <div class="progress double11-2019-progress">
                  <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0">
                  </div>
                </div>
                <%= link_to "立即抢购", Rails.configuration.portal_host + section[:link], title: kodo[:title] + " - 立即抢购", class: "btn btn-lg btn-double11-2019 disabled", target: "_blank" -%>
              </section>
              <% end -%>
            </div>
          </div>
        </div>
      </div>
      <% end -%>
    </div>
  </div>
</div>

