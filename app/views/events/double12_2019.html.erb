<%# 存储专场 %>
<div class="features features-double12-2019 features-double12-2019-kodo tab-content-double12-2019-kodo">
  <div class="container">
    <%# 拉新包 %>
    <div class="intro">
      <h2 class="title">
        <%= image_tag "double12_2019/icon-title-red.png", title: I18n.t("views.events.double12_2019.kodo.title"), alt: I18n.t("views.events.double12_2019.kodo.title"), class: "icon-title left" -%>
        <%= I18n.t("views.events.double12_2019.kodo.title") -%>
        <%= image_tag "double12_2019/icon-title-red-blue.png", title: I18n.t("views.events.double12_2019.kodo.title"), alt: I18n.t("views.events.double12_2019.kodo.title"), class: "icon-title right" -%>
      </h2>
    </div>
    <div class="row">
      <div class="col-md-12 col-sm-12 col-xs-12 col-table">
        <div class="table-container">
          <table class="kodo-top-table" width="100%" border="0" cellpadding="0" cellspacing="0">
            <tbody>
              <% Rails.configuration.double12_2019_package[:kodo][:table].each do |table| -%>
              <tr>
                <td class="padding width-30">
                  <span class="activity-tip <%= table[:tipClass] -%>"><%= table[:tipText] -%></span>
                  <p class="title"><%= table[:title] -%></p>
                </td>
                <td class="width-10">
                  <p class="sub-title">时长</p>
                  <p class="desc"><%= table[:duration] -%></p>
                </td>
                <td class="width-15">
                  <p class="sub-title">地区</p>
                  <div class="content-dropdown">
                    <select class="form-control double12-2019-dropdown double12-2019-kodo-top-dropdown">
                      <% table[:areas].each do |area| -%>
                      <option value="<%= area[:value] -%>"><%= area[:text] -%></option>
                      <% end -%>
                    </select>
                  </div>
                </td>
                <% table[:sections].each do |section| -%>
                <td class="text-center width-20 content-section <%= section[:class] -%>" id="<%= section[:id] + "-price" -%>">
                  <div class="content-price">
                    <div class="row">
                      <div class="col-md-5 col-sm-5 col-xs-5 col-price col-price-left">
                        <span class="span-red span-bold">￥</span><span class="span-red span-huge span-bold"><%= section[:price] -%></span>
                      </div>
                      <div class="col-md-7 col-sm-7 col-xs-7 col-price col-price-right">
                        <span class="span-red">节省&nbsp;<%= section[:save] -%>&nbsp;元</span>
                        <span class="span-red span-elide">原价&nbsp;<%= section[:originPrice] -%>&nbsp;元</span>
                      </div>
                    </div>
                  </div>
                </td>
                <td class="text-center width-15 content-section <%= section[:class] -%>" id="<%= section[:id] + "-btn" -%>">
                  <%= link_to section[:btnText], signin_url('www', '/events/20191212'), title: table[:title] + " - " + section[:btnText], class: "btn btn-lg btn-double12-2019 package-buy disabled " + section[:btnClass], target: "_blank", data: { package_id: section[:packageID], package_info: section[:info] } -%>
                </td>
                <% end -%>
              </tr>
              <% end -%>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <%# 华南专区 %>
    <div class="intro">
      <p class="sub-title high-light-1">
        <%= I18n.t("views.events.double12_2019.kodo.southSubTitle") -%>
      </p>
      <p class="desc"><%= raw(I18n.t("views.events.double12_2019.kodo.desc")) -%></p>
    </div>
    <div class="row">
      <% Rails.configuration.double12_2019_package[:kodo][:southList].each do |kodo| -%>
