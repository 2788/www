<!-- channel Modal -->
    <div class="modal fade alert-modal-css form-wrapper" id="channel-modal" tabindex="-1" role="dialog" aria-labelledby="channel-modal-label" data-keyboard="false" data-backdrop="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="channel-modal-label">七牛云渠道代理合作申请表</h4>
          </div>
          <div class="modal-body">
            <div id="channel-alert" class="alert alert-success submit-alert" style="display: none;"></div>


            <%= form_for ChannelCooperation.new, url: {controller: "cooperations", action: "create_channel", t: SecureRandom.uuid}, :remote => true, html: {class: "form-horizontal", id: "channel-form"} do |f| %>
              <%= f.hidden_field :referer, value: request.original_url -%>

              <div class="form-group">
                <label for="channel-company" class="col-sm-2 control-label">公司名称<small class="text-danger">*</small></label>
                <div class="col-sm-10">
                  <%= f.text_field :company, class: "form-control", id:"channel-company", validate: true %>
                  <div class="div-error">
                    <label for="" class="error-label">请填写公司名称</label>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <label for="channel-website" class="col-sm-2 control-label">公司官网<small class="text-danger">*</small></label>
                <div class="col-sm-10">
                  <%= f.text_field :website, class:"form-control", id:"channel-website", validate: true %>
                  <div class="div-error">
                    <label for="" class="error-label">请填写公司官网</label>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <label for="channel-business" class="col-sm-2 control-label">主营业务<small class="text-danger">*</small></label>
                <div class="col-sm-10">
                  <%= f.text_field :business, class:"form-control", id:"channel-business", validate: true %>
                  <div class="div-error">
                    <label for="" class="error-label">请填写主营业务</label>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <label for="channel-name" class="col-sm-2 control-label">姓名<small class="text-danger">*</small></label>
                <div class="col-sm-10">
                  <%= f.text_field :name, class:"form-control", id:"channel-name", validate: true %>
                  <div class="div-error">
                    <label for="" class="error-label">请填写联系人姓名</label>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <label for="channel-phone" class="col-sm-2 control-label">联系电话<small class="text-danger">*</small></label>
                <div class="col-sm-10">
                  <%= f.telephone_field :phone, class:"form-control", id:"channel-phone", validate: true %>
                  <div class="div-error">
                    <label for="" class="error-label">请填写联系电话</label>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <label for="channel-im" class="col-sm-2 control-label">QQ<small class="text-danger">*</small></label>
                <div class="col-sm-10">
                  <%= f.text_field :im, class:"form-control", id:"channel-im", validate: true %>
                  <div class="div-error">
                    <label for="" class="error-label">请以填写 QQ 号</label>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <label for="channel-email" class="col-sm-2 control-label">E-mail<small class="text-danger">*</small></label>
                <div class="col-sm-10">
                  <%= f.text_field :email, type: "email", class:"form-control", id:"channel-email", validate: true %>
                  <div class="div-error">
                    <label for="" class="error-label">请填写 E-mail</label>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <label for="channel-intention" class="col-sm-2 control-label">合作意向</label>
                <div class="col-sm-10">
                  <%= f.text_area :intention, class: "form-control", id:"channel-intention" %>
                </div>
              </div>

              <div class="actions">
                <%= f.button "提 交", class:"btn btn-primary", id:"channel-submit", disabled: true, data: {disable_with: "正在提交中..."}  %>
              </div>
            <% end %>
          </div>
          <div class="modal-footer alert alert-info" role="alert">感谢您对七牛产品的关注和信任，我们期待您成为七牛云的渠道代理合作伙伴。在表单填写过程中有任何问题欢迎联系 marketing@qiniu.com，邮件标题注明【渠道合作】，会有专人与您联系。</div>
        </div>
      </div>
    </div>

