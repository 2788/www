<%# 智能多媒体服务专区 %>
<div class="features features-spring-sale spring-sale features-spring-sale-dora tab-content-spring-sale-dora">
  <div class="container">
    <div class="intro">
      <div class="title-wrapper">
        <div class="line hidden-xs"></div>
        <h2 class="title">
          <%= image_tag "spring_sale/dora_hot.png", title: I18n.t("views.events.spring_sale.dora.title"), alt: I18n.t("views.events.spring_sale.dora.title"), class: "icon-title" -%>
          <%= I18n.t("views.events.spring_sale.dora.title") -%>
        </h2>
        <div class="line hidden-xs"></div>
      </div>
      <p class="desc"><%= raw(I18n.t("views.events.spring_sale.dora.desc")) -%></p>
    </div>
    <div class="dora-deepbg-container">
      <h3 class="sub-title"><%= Rails.configuration.spring_sale_package[:dora][:h264][:title] -%></h3>
      <div class="dora-nav-container row">
        <% Rails.configuration.spring_sale_package[:dora][:h264][:tabs].each do |tab| -%>
        <div class="col-md-4 col-sm-4 col-xs-4">
          <div class="dora-nav <%= tab[:class] -%>" id="<%= tab[:id] -%>"><%= tab[:text] -%></div>
        </div>
        <% end -%>
      </div>
      <div class="row">
        <% Rails.configuration.spring_sale_package[:dora][:packages264].each do |dora| -%>
        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12 col-item col-kodo">
          <div class="thumbnail">
            <div class="caption-sale">
              <div class="product-header">
                <h4 class="title"><%= dora[:title] -%></h4>
                <div class="tag">开春特惠</div>
                <p class="desc"></p>
              </div>
              <div class="content-container">
                <p class="props"><%= dora[:spec] -%></p>
                <% dora[:sections].each do |section| -%>
                <section class="content-section <%= section[:class] -%>" id="<%= section[:id] -%>">
                  <div class="content-price">
                    <div class="row">
                      <div class="col-md-6 col-sm-6 col-xs-6 col-price col-price-left">
                        <span class="span-red span-bold">￥</span><span class="span-red span-huge span-bold"><%= section[:price] -%></span>
                      </div>
                      <div class="col-md-6 col-sm-6 col-xs-6 col-price col-price-right">
                        <span class="span-red">节省&nbsp;<%= section[:save] -%>&nbsp;元</span>
                        <span class="span-red span-elide">原价&nbsp;<%= section[:originPrice] -%>&nbsp;元</span>
                      </div>
                    </div>
                  </div>
                  <div class="buy-btn">
                    <%= link_to "立即抢购", signin_url('www', '/events/20200316'), title: dora[:title] + " - 立即抢购", class: "btn btn-lg btn-double12-2019 package-buy disabled", target: "_blank", data: { package_id: section[:packageID], package_info: section[:info] } -%>
                  </div>
                </section>
                <% end -%>
              </div>
            </div>
          </div>
        </div>
        <% end -%>
      </div>
    </div>
    <div class="dora-deepbg-container">
      <h3 class="sub-title"><%= Rails.configuration.spring_sale_package[:dora][:h265][:title] -%></h3>
      <div class="dora-nav-container row">
        <% Rails.configuration.spring_sale_package[:dora][:h265][:tabs].each do |tab| -%>
        <div class="col-md-4 col-sm-4 col-xs-4">
