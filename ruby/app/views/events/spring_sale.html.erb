                  <div class="content-price">
                    <div class="row">
                      <div class="col-md-6 col-sm-6 col-xs-6 col-price col-price-left">
                        <span class="span-red span-bold">￥</span><span class="span-red span-huge span-bold"><%= section[:price] -%></span>
                      </div>
                      <div class="col-md-6 col-sm-6 col-xs-6 col-price col-price-right">
                        <span class="span-red">节省&nbsp;<%= section[:save] -%>&nbsp;元</span>
                        <span class="span-red span-elide">原价&nbsp;<%= section[:originPrice] -%>&nbsp;元</span>
                      </div>
                    </div>
                  </div>
                  <div class="buy-btn">
                    <%= link_to "立即抢购", signin_url('www', '/events/20200316'), title: dora[:title] + " - 立即抢购", class: "btn btn-lg btn-double12-2019 package-buy disabled", target: "_blank", data: { package_id: section[:packageID], package_info: section[:info] } -%>
