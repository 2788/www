    <div class="dora-deepbg-container dora-table-container">
      <table class="dora-table" width="100%" border="0" cellpadding="0" cellspacing="0">
        <thead>
          <tr>
            <th>类型</th><th>次数/时间</th><th>折扣</th><th>价格</th><th></th>
          </tr>
        </thead>
        <tbody>
          <% Rails.configuration.spring_sale_package[:dora][:tables].each do |table| -%>
          <tr>
            <td class="type"><%= table[:type] -%></td>
            <td class="duration"><%= table[:duration] -%></td>
            <td class="discount"><span class="discount-tag"><%= table[:discount] -%></span></td>
            <td class="price"><%= table[:price] -%> 元</td>
            <td class="text-center">
