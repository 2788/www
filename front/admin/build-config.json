{
  "extends": "typescript-react",
  "srcDir": "src",
  "publicUrl": "https://static-file.qiniu.io/",
  "entries": {
    "index": "src/index.tsx"
  },
  "pages": {
    "index": {
      "template": "src/index.ejs",
      "entries": "index",
      "path": "^\\/"
    }
  },
  "transforms": {
    "md": "file"
  },
  "devProxy": {
    "/api": "http://admin-backend-www-env-staging.jfcs-k8s-qa1.qiniu.io"
  },
  "deploy": {
    "config": {
      "accessKey": "{{process.env.BUILD_DEPLOY_ACCESS_KEY}}",
      "secretKey": "{{process.env.BUILD_DEPLOY_SECRET_KEY}}",
      "bucket": "admin-v2-static"
    }
  },
  "engines": {
    "builder": "^2.7.1"
  }
}
