{
  "extends": "typescript-react",
  "srcDir": "src",
  "publicUrl": "https://qiniu.staticfile.org/",
  "optimization": {
    "transformDeps": ["tr46", "react-hot-loader", "ansi-regex", "whatwg-url", "webidl-conversions"]
  },
  "targets": {
    "browsers": [
      "IE >= 10",
      "Chrome >= 46",
      "Firefox ESR",
      "last 2 Safari versions"
    ]
  },
  "entries": {
    "index": "src/index.tsx"
  },
  "pages": {
    "index": {
      "template": "src/index.ejs",
      "entries": "index",
      "path": "^\\/marketing\\/"
    }
  },
  "transforms": {
    "less": {
      "transformer": "less",
      "config": {
        "options": {
          "javascriptEnabled": true
        },
        "modules": false
      }
    },
    "m.less": {
      "transformer": "less",
      "config": {
        "options": {
          "javascriptEnabled": true
        },
        "modules": true
      }
    },
    "svg@less": "file",
    "svg@m.less": "file"
  },
  "devProxy": {
    "/api": "http://www-dev.qiniu.io",
    "/signin": {
      "target": "http://www-dev.qiniu.io",
      "pathRewrite": {
        "^/signin": "/signin"
      }
    }
  },
  "// TODO: deploy": "add www marketing deploy config",
