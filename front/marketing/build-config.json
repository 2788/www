{
  "extends": "typescript-react",
  "srcDir": "src",
  "publicUrl": "https://static.qiniu.com/",
  "optimization": {
    "transformDeps": ["tr46", "react-hot-loader", "ansi-regex", "whatwg-url", "webidl-conversions"],
    "extractCommon": false,
    "extractVendor": false
  },
  "targets": {
    "browsers": [
      "IE >= 10",
      "Chrome >= 46",
      "Firefox ESR",
      "last 2 Safari versions"
    ]
  },
  "entries": {
    "www-marketing": "src/index.tsx"
  },
  "pages": {
    "index": {
      "template": "src/index.ejs",
      "entries": "www-marketing",
      "path": "^\\/activity\\/"
    }
  },
  "transforms": {
    "less": {
      "transformer": "less",
      "config": {
        "options": {
          "javascriptEnabled": true
        },
        "modules": false
      }
    },
    "m.less": {
      "transformer": "less",
      "config": {
        "options": {
          "javascriptEnabled": true
        },
        "modules": true
      }
    },
    "m.css": {
      "transformer": "css",
      "config": {
        "modules": true
      }
    },
    "svg@less": "file",
    "svg@m.less": "file",
    "svg@m.css": "file"
  },
  "devProxy": {
    "/api": "http://www-janus-koderover-staging-www.cs-spock.cloudappl.com"
  },
  "deploy": {
    "config": {
      "accessKey": "{{process.env.BUILD_DEPLOY_ACCESS_KEY}}",
      "secretKey": "{{process.env.BUILD_DEPLOY_SECRET_KEY}}",
      "bucket": "portal-v4-static"
    }
  },
  "engines": {
    "builder": "1.16.1"
  },
  "envVariables": {}
}
