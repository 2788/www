    "config": {
      "accessKey": "tkabCgtBh5OiAbZ32-BC3KbLpWO274oY4nL6e5hr",
      "secretKey": "1oUlrJlpJprkl4sLZQYdJLsGoVTCZaZXEbmiZTNh",
      "bucket": "portal-v4-static"
    }
  },
  "engines": {
    "builder": "1.15.1"
  },
  "envVariables": {}
}
