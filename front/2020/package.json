    "build:sitemap": "node sitemap.js",
    "build:externals": "node externals.js",
    "build": "npm run clean && npm run prepare:initial-data && NODE_OPTIONS=\"--max_old_space_size=4096\" next build && next export && npm run build:sitemap && npm run build:externals",
    "start": "next start",
    "lint": "tsc --noEmit && eslint --ignore-path .gitignore --ext .ts,.tsx ./",
    "test": "jest --coverage",
    "travis:script": "npm run lint && npm test",
    "travis:before-script": "yarn"
